*{
	margin: 0;
	padding: 0;
}
.footer_wrap{
	height: 348px;
	min-width: 1100px;
	background: url(../image/yinyue_bg.jpg);
}
.footer{
	width: 1100px;
	margin: 0 auto;
}
.music{
	padding: 116px 0 0 276px;
}

.music span{
	float: left;
	cursor: pointer;
	width: 57px;
	height: 57px;
	background: url(../image/yinyue_duo.png) no-repeat;
	margin-right: 26px;
}
.music span:hover{
	background-position:0px -57px;
	animation: 1s linear 0s normal none 1 running su-pudding;
}
.footer .shenmin{
	border-top: 2px dashed #9890a2;
	text-align: center;
	line-height: 65px;
	margin-top: 79px;
	color: #fff;
	cursor: pointer;
	
}
.footer .shenmin:hover{
	color: #6cf;
}
